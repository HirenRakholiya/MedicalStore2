
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="container">
    <table border="1" class="table">
        <tr class="bg-success">
            <th>Category Name</th>
        </tr>

        @for(int i=0;i<Model.Rows.Count;i++)
            {
            <tr>
                <td>@Model.Rows[i][1]</td>
                </tr>
            }
    </table>
</div>