
@{
    ViewBag.Title = "Index";
}



<h2>Manufacture</h2>
<a href="@Url.Action("Create","Manufacture")">Add Manufacture</a>
<div class="container-fluid">
    <table class="table">
        <tr class="bg-primary">
            <th> Name</th>
            <th> Email</th>
            <th> Mobile</th>
            <th> Address</th>
            <th> Zipcode</th>
            <th> Country</th>
            <th colspan="2">Action</th>
        </tr>

        @for (int i = 0; i < Model.Rows.Count; i++)
        {
    <tr>
        <td>@Model.Rows[i][1]</td>
        <td>@Model.Rows[i][2]</td>
        <td>@Model.Rows[i][3]</td>
        <td>@Model.Rows[i][4]</td>
        <td>@Model.Rows[i][5]</td>
        <td>@Model.Rows[i][6]</td>
        <td style="width:200px">
            <button class="btn bg-success border-0" style="width:80px"><a href="@Url.Action("Edit","Category",new { @id=@Model.Rows[i][0]})">Edit</a></button>
            <button class="btn bg-danger border-0" style="width:80px"> <a href="@Url.Action("Delete","Category",new { @id=@Model.Rows[i][0]})">Delete</a></button>
        </td>
    </tr>
        }
    </table>
</div>
